{
  "name": "backend",
  "version": "1.0.0",
  "description": "AI-powered job search platform backend for South Africa",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "workers": "tsx src/workers/workerManager.ts",
    "workers:prod": "node dist/workers/workerManager.js",
    "workers:dev": "tsx watch src/workers/workerManager.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "node scripts/setup-test-db.js && jest --testMatch='**/*.integration.ts' --runInBand",
    "test:unit": "jest --testMatch='**/*.test.ts' --testPathIgnorePatterns='integration'",
    "test:setup": "node scripts/setup-test-db.js",
    "test:all": "npm run test:setup && jest --runInBand",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx src/prisma/seed.ts",
    "prisma:deploy": "prisma migrate deploy",
    "test:deployment": "tsx scripts/test-huggingface-deployment.js",
    "test:deployment:quick": "tsx scripts/test-deployment-quick.js",
    "test:deployment:timeout": "tsx scripts/test-deployment-timeout.js",
    "models:deploy": "tsx -e \"import('./src/init/model-deployment.init.js').then(m => m.initializeModelsOnStartup())\"",
    "models:test": "npm run test:deployment"
  },
  "keywords": [
    "job-search",
    "ai",
    "south-africa",
    "employment"
  ],
  "author": "AI Job Chommie Team",
  "license": "MIT",
  "dependencies": {
